}

.titleimg-frame {
  display: flex;
  width: 30%;
  height: 100%;
  margin-right: 20rpx;
  margin-top: 15rpx;
}

.titlesection {
  display:flex;
  flex-direction: row;
  height: 150rpx;
  width: 100%;
}

.fullname-name {
  font-size: 34rpx;
  font-family: Microsoft Yahei;
  font-weight: bold;
}

